    </collision>
    <visual name="footprint1_vis">
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.98 0.6 0.215"/>
      </geometry>
    </visual>
  </link>
  
  <joint name="joint2" type="fixed">
    <parent link="footprint1"/>
    <child link="footprint2"/>
    <origin xyz="-0.53 0.0 0.0" rpy="0.0 0.0 0.0"/>
